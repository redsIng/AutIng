  Cinematica diretta:

  <\session|maxima|default>
    <\unfolded-io>
      <with|color|red|(<with|math-font-family|rm|%i>21) >
    <|unfolded-io>
      Q[polsosferico](q1,q2,q3,d3):=

      \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ Q(q1,0,-%pi/2,0).

      \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ Q(q2,0,%pi/2,0).

      \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ Q(q3,d3,0,0)

      ;
    <|unfolded-io>
      \;

      \ <math|<with|math-display|true|<text|<with|font-family|tt|color|red|(<with|math-font-family|rm|%o21>)
      >>Q<rsub|<math-up|polsosferico>><around|(|<with|math-font-family|rm|q1>,<with|math-font-family|rm|q2>,<with|math-font-family|rm|q3>,<with|math-font-family|rm|d3>|)>\<assign\>Q<around*|(|<with|math-font-family|rm|q1>,0,<frac|-\<pi\>|2>,0|)>\<cdot\>Q<around*|(|<with|math-font-family|rm|q2>,0,<frac|\<pi\>|2>,0|)>\<cdot\>Q<around*|(|<with|math-font-family|rm|q3>,<with|math-font-family|rm|d3>,0,0|)>>>
    </unfolded-io>

    <\unfolded-io>
      <with|color|red|(<with|math-font-family|rm|%i>22) >
    <|unfolded-io>
      Qpolsosferico:Q[polsosferico](q[1],q[2],q[3],d[3]);
    <|unfolded-io>
      \;

      \ <math|<with|math-display|true|<text|<with|font-family|tt|color|red|(<with|math-font-family|rm|%o22>)
      >><matrix|<tformat|<table|<row|<cell|cos <around*|(|q<rsub|1>|)>*cos
      <around*|(|q<rsub|2>|)>*cos <around*|(|q<rsub|3>|)>-sin
      <around*|(|q<rsub|1>|)>*sin <around*|(|q<rsub|3>|)>>|<cell|-cos
      <around*|(|q<rsub|1>|)>*cos <around*|(|q<rsub|2>|)>*sin
      <around*|(|q<rsub|3>|)>-sin <around*|(|q<rsub|1>|)>*cos
      <around*|(|q<rsub|3>|)>>|<cell|cos <around*|(|q<rsub|1>|)>*sin
      <around*|(|q<rsub|2>|)>>|<cell|d<rsub|3>*cos
      <around*|(|q<rsub|1>|)>*sin <around*|(|q<rsub|2>|)>>>|<row|<cell|cos
      <around*|(|q<rsub|1>|)>*sin <around*|(|q<rsub|3>|)>+sin
      <around*|(|q<rsub|1>|)>*cos <around*|(|q<rsub|2>|)>*cos
      <around*|(|q<rsub|3>|)>>|<cell|cos <around*|(|q<rsub|1>|)>*cos
      <around*|(|q<rsub|3>|)>-sin <around*|(|q<rsub|1>|)>*cos
      <around*|(|q<rsub|2>|)>*sin <around*|(|q<rsub|3>|)>>|<cell|sin
      <around*|(|q<rsub|1>|)>*sin <around*|(|q<rsub|2>|)>>|<cell|d<rsub|3>*sin
      <around*|(|q<rsub|1>|)>*sin <around*|(|q<rsub|2>|)>>>|<row|<cell|-sin
      <around*|(|q<rsub|2>|)>*cos <around*|(|q<rsub|3>|)>>|<cell|sin
      <around*|(|q<rsub|2>|)>*sin <around*|(|q<rsub|3>|)>>|<cell|cos
      <around*|(|q<rsub|2>|)>>|<cell|d<rsub|3>*cos
      <around*|(|q<rsub|2>|)>>>|<row|<cell|0>|<cell|0>|<cell|0>|<cell|1>>>>>>>
    </unfolded-io>

